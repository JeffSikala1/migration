export IFS=$'\n' ;for i in $(sqlite3 /app/ConexusSFTP/maps/pathways.db -header "select srcfile from copiedfiles"|cut -d'/' -f6) ; do echo -- $i --; var=$(ls -l /NSFS_NAS/database/cparchive/2021/04/13|grep "$i"); if [ "$?" -eq 0 ]; then echo "$i" >> alreadyarchived; fi;  done
for i in `cat alreadyarchived `; do echo "delete from copiedfiles where srcfile like '%$i%';"; done > archiveddeletes
