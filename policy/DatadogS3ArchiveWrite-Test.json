{
  "Version": "2012-10-17",
  "Statement": [
    { "Sid": "ArchiveWrite",
      "Effect": "Allow",
      "Action": ["s3:PutObject"],
      "Resource": "arn:aws:s3:::elp-logs-test-cnxs/ddog/test/*"
    },
    { "Sid": "ListAndReadForRehydrate",
      "Effect": "Allow",
      "Action": ["s3:GetObject", "s3:ListBucket"],
      "Resource": [
        "arn:aws:s3:::elp-logs-test-cnxs",
        "arn:aws:s3:::elp-logs-test-cnxs/ddog/test/*"
      ]
    }
  ]
}